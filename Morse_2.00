#include <Keyboard.h>
#define Ton_time 300
#define Tu_time 700
void setup() {
  // put  setup code here, to run once:
  Serial.begin(9600);
  pinMode(12, INPUT);
}
unsigned int t;
unsigned int t1;
unsigned int t2=0;
int a1;

void loop() {
  if (digitalRead(12) == 0) {

    t = millis();//set timer
    while (1) {//Count bttom doun time
        if(digitalRead(12)==1){
          break;
        }
    }

    t1=millis()-t;

    t1=t;
    if((millis()-t1)>50&&(millis()-t1)<Ton_time){
      Serial.print("ãƒ»");
    }else if((millis()-t1)<Tu_time&&(millis()-t1)>10){
      Serial.print("-");
    }

    t2=millis();
    a1=1;

  }else if(millis()-t2>900&&a1==1){
      Serial.println(" ");
      a1=0;
  }
}
